<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.janmejay.meete.controller.Home.mainActivity">
        <include
            android:id="@+id/rl_header"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            layout="@layout/toolbar_people"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"></include>

    <RelativeLayout
        android:id="@+id/rl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@id/rl_footer"
        android:layout_below="@id/rl_header"></RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_footer"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_alignParentBottom="true">
        <View
            android:background="#b7b7b7"
            android:layout_width="match_parent"
            android:layout_height="1dp"></View>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_3" />

        <ImageView
            android:onClick="btnClickMap"
            android:id="@+id/iv_map"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_20"
            android:src="@mipmap/map" />

        <ImageView
            android:onClick="btnNearBy"
            android:id="@+id/iv_list_places"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_toRightOf="@id/iv_map"
            android:src="@drawable/list" />

        <ImageView
            android:onClick="btnlistpeople"
            android:id="@+id/iv_list_people"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_toRightOf="@id/iv_list_places"
            android:src="@drawable/group" />

        <ImageView
            android:onClick="btnHistory"
            android:id="@+id/iv_history"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_toRightOf="@id/iv_list_people"
            android:src="@drawable/hist" />

        <ImageView
            android:id="@+id/iv_notification"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_toRightOf="@id/iv_history"
            android:src="@drawable/noti" />
    </RelativeLayout>
</RelativeLayout>
